{
  "name": "@fedify/denokv",
  "version": "1.10.3",
  "license": "MIT",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "es-toolkit": "npm:es-toolkit@^1.30.0"
  },
  "exports": {
    ".": "./src/mod.ts"
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "publish": {
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check src/*.ts",
