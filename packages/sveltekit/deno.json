{
  "name": "@fedify/sveltekit",
  "version": "1.10.3",
  "license": "MIT",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@sveltejs/kit": "npm:@sveltejs/kit@^2.0.0"
  },
  "exports": {
    ".": "./src/mod.ts"
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "publish": {
    "exclude": [
      "**/*.test.ts",
      "tsdown.config.ts"
    ]
  },
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check src/*.ts",
    "test": "deno test --allow-net --allow-env"
  }
}
